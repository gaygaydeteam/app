{
  "expo": {
    "name": "HKT",
    "slug": "hkt",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.skychain.hkt"
    },
    "android": {
      "package": "com.skychain.hkt"
    },
    "androidStatusBar": {
        "barStyle": "light-content"
    }
  }
}
